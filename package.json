{
  "name": "jsreport-text",
  "version": "1.0.1",
  "description": "jsreport recipe for rendering plain text from html",
  "keywords": [
    "jsreport"
  ],
  "scripts": {
    "test": "standard",
    "build": "jsreport-studio-build",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish"
  },
  "standard": {
    "ignore": [
      "main.js"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  },
  "homepage": "https://github.com/jsreport/jsreport-text",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-text.git"
  },
  "license": "LGPL",
  "dependencies": {
    "in-publish": "2.0.0"
  },
  "author": "Jan Blaha",
  "devDependencies": {
    "babel-eslint": "8.2.1",
    "eslint": "4.17.0",
    "eslint-config-standard-jsx": "4.0.2",
    "eslint-plugin-babel": "4.1.2",
    "jsreport-studio-dev": "1.1.2",
    "standard": "10.0.3"
  },
  "files": [
    "lib",
    "studio",
    "index.js",
    "jsreport.config.js"
  ],
  "engines": {
    "node": ">=8.0.0"
  }
}
